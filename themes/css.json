{
    "$schema": "vscode://schemas/color-theme",
    "name": "Kiki CSS Colors",
    "tokenColors": [
        {
            "scope": [
                "entity.name.tag.css",
            ],
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": [
                "support.type.property-name.css",
            ],
            "settings": {
                "foreground": "#9CDCFE"
            }
        },
        {
            "scope": [
                "constant.numeric.css",
            ],
            "settings": {
                "foreground": "#b5cea8"
            }
        },
        {
            "scope": [
                "constant.other.color.rgb-value.hex.css",
            ],
            "settings": {
                "foreground": "#afff83"
            }
        },
        {
            "scope": [
                "support.constant.property-value.css",
            ],
            "settings": {
                "foreground": "#569cd6"
            }
        }
    ],
}